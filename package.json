{
  "name": "dialogflow-import",
  "version": "0.0.1",
  "description": "Utility library to import agent backup files from Google DialogFlow",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest --coverage",
    "lint": "eslint 'src/**/*.ts' --quiet --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edupsousa/dialogflow-import.git"
  },
  "keywords": [
    "typescript",
    "dialogflow",
    "backup",
    "import",
    "agent",
    "intents",
    "entities",
    "training",
    "phrases"
  ],
  "author": "Eduardo P de Sousa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/edupsousa/dialogflow-import/issues"
  },
  "homepage": "https://github.com/edupsousa/dialogflow-import#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix"
    ]
  },
  "files": [
    "lib/**/*"
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.3",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "jszip": "^3.6.0"
  }
}
